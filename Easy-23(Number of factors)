import java.util.Scanner;
class factors
{
	public static void main(String args[])
	{
		Scanner input=new Scanner(System.in);
		System.out.print("Enter the number:");
		int n=input.nextInt();
		int factor=0;
		for(int i=1;i<=n;i++)
		{
			if(n%i==0)
			{
				factor=factor+1;
			}
		}
		System.out.print("Number of factors for the number:"+factor);
	}
}
